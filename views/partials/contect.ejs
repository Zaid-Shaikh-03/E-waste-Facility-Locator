<!-- Contect  -->
<div id="contect">
  <div class="getin">
    <h2>Get in touch</h2>
    <p class="fw-bold">
      Looking for help? Fill the form and start a new adventure
    </p>

    <div class="getin-details">
      <h3>Headquaters</h3>
      <div>
        <i class="fas fa-home get"></i>
        <p>744 New York Ave, Brooklyn, Kings, New York 10244</p>
      </div>
      <h3>Phone</h3>
      <div>
        <i class="fas fa-phone get"></i>
        <p>
          (+642) 245 356 432 <br />
          (+420) 336 476 328
        </p>
      </div>
      <h3>Support</h3>
      <div>
        <i class="fa fa-envelope-open-text get"></i>
        <p>
          xyz123@Support.com <br />
          help@e-commerce.com
        </p>
      </div>
      <h3>Support</h3>
      <div class="pro-links">
        <i class="fab fa-facebook-f"></i>
        <i class="fab fa-instagram"></i>
        <i class="fab fa-linkedin-in"></i>
        <i class="fab fa-behance"></i>
      </div>
    </div>
  </div>
  <div class="form">
    <h4 class="fw-bolder">Let's Connect</h4>
    <p class="fw-bold">
      Lorem ipsum dolor sit amet consectetur, adipisicing elit.
    </p>
    <div class="form-row">
      <input type="text" placeholder="Your Name" />
      <input type="text" placeholder="Email" />
    </div>
    <div class="form-col">
      <input type="text" placeholder="Subject" />
    </div>
    <div class="form-col">
      <textarea
        name=""
        id=""
        cols="30"
        rows="8"
        placeholder="How can we help?"
      ></textarea>
    </div>
    <div class="form-col">
      <button>Send Message</button>
    </div>
  </div>
</div>
